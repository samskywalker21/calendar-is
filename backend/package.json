{
	"dependencies": {
		"concurrently": "^9.1.0",
		"cors": "^2.8.5",
		"express": "^4.19.2",
		"mongoose": "^8.3.1",
		"nodemon": "^3.1.0"
	},
	"devDependencies": {
		"prettier": "3.3.3"
